<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>UTSME: Telemetry</title>
  <meta name="viewport" content="width=device-width">
  <link rel="stylesheet" href="stylesheets/style.css" type="text/css">

</head>

<body>
  <header>
    <h1 class="title">UTS Motorsports 2019</h1>
    <button type="button" class="home" onclick="location.href='http://localhost:3000'">Home</button>
    <button type="button" class="mechanical" onclick="location.href='http://localhost:3000/mechanical'">Mechanical</button>
    <button type="button" class="electrical" onclick="location.href='http://localhost:3000/electrical'">Electrical</button>
    <button type="button" class="start-up" onclick="location.href='http://localhost:3000/start-up'">Start Up</button>
</header>

  <div class="steer">
    <div class="steer-label">Steer Pos</div>
    <div class="steer-min">-90</div>
    <div class="steer-max">90</div>
    <label class="steer-num" id="steer-num"> 0 </label>
  </div>

  <div class="gauge">
    <label class="label">Brake Pressure</label>
    <div class="brake-number">0</div>
  </div>

  <div class="gauge">
    <table>
        <tr>
          <label class="label"> Down Force</label>
            <td class="df-fault-1">FL</td>
            <td class="df-fault-2">FR</td>
            <td class="df-fault-3">BL</td>
            <td class="df-fault-4">BK</td>
        </tr>
    </table>
  </div>

<div class="gauge">
  <table>
    <tr>
      <label class="label">Faults</label>
      <td class="fault-1">F1</td>
      <td class="fault-2">F2</td>
      <td class="fault-3">F3</td>
      <td class="fault-4">F4</td>
    </tr>
  </table>
</div>

<div class="gauge">
  <div class="label">BMS Current</div>
  <div class="bms-number">0</div>
</div>
<div class="gauge">
  <div class="label">Speedometer</div>
  <div class="sc-background" id="speed">
      <div class="sc-percentage" id=""></div>
      <div class="sc-mask"></div>
      <span class="sc-value" id="g1">100</span>
    </div>
    <span class="sc-min">0</span>
    <span class="sc-max">100</span>
</div>

<div class="gauge">
  <div class="label">Acc Voltage</div>
  <div class="sc-background">
      <div class="sc-percentage" id=""></div>
      <div class="sc-mask"></div>
      <span class="sc-value" id="g1">100</span>
    </div>
    <span class="sc-min">0</span>
    <span class="sc-max">1000</span>
</div>

<div class="gauge">
  <div class="label">Temperature</div>
  <div class="sc-background">
      <div class="sc-percentage" id=""></div>
      <div class="sc-mask"></div>
      <span class="sc-value" id="g1">100</span>
    </div>
    <span class="sc-min">30</span>
    <span class="sc-max">90</span>
</div>



</body>
<script src="scripts/require.js"></script>
<script src="scripts/gui.js" type="text/javascript"></script>

</html>
